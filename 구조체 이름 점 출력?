#pragma warning (disable : 4996)
#include<stdio.h>
#include<stdlib.h>

typedef struct _p
{
	char name[20];
	int x;
	int y;
}Point;

int main()
{
	Point p1, p2;
	strcpy(p1.name, " 구조체 복사 ");
	p1.x = 10;
	p1.y = 20;

	strcpy(p2.name, p1.name);
	p2.x = p1.x;
	p2.y = p1.y;

	printf("%s\n", p2.name);
	printf("%d %d\n", p2.x, p2.y);
	return 0;
}



#pragma warning (disable : 4996)
#include<stdio.h>
#include<stdlib.h>

typedef struct _p
{
	char name[20];
	int x;
	int y;
}Point;

int main()
{
	Point p1, p2;
	strcpy(p1.name, " 구조체 복사 ");
	p1.x = 10;
	p1.y = 20;

	memcpy(&p2, &p1, sizeof(Point));

	printf("%s\n", p2.name);
	printf("%d %d\n", p2.x, p2.y);
	return 0;
}
