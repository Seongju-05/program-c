#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main()
{
 //구구단 2~9단 출력하는 코드르 작성해 주세여.
 /*
 int dan = 2, a = 0;
 for (dan = 9; dan > 1; dan--) {
  printf("====================\n");
  for (a = 9; a > 0; a--);
  printf("%d * %d = %d\n",dan, a, dan * a);
 }


#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main ()
{
int a, b;
 int sum = 0, area = 1;
 printf("두 숫자를 입력하세요\n");
 scanf ("%d %d", &a, &b);
 for (; a <= b; a++)
 {
  sum = sum + a;
  area = area *a;
 }
  printf("%d  %d\n", sum, area);
  return 0;
  }
