// 3항 연사자를 이용한 프로그램 조건 ? 명령A(참) : 명령B(거짓)
#include <stdio.h>
int main()
{
	int score, pass;
	printf("점수를 입력하세요 : ");
	scanf("%d", &score);
	printf("합격점을 입력하세요");
	scanf("%d", &pass);
  score > pass ? printf("합격") : printf("불합격"); // = printf("%s",score >pass ? "합격" : "불합격");
}
