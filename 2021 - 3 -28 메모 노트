#pragma warning (disable : 4996)
#include <stdio.h>


int main()
{
//1~200까지 3의 배수이면서 5의 배수인 숫자를 출력하시오.(5개 마다 세줄에서 출력)
int a, cnt = 1;
  for (a = 1; a <= 200; a++)
     if (a % 3  == 0 && a % 5 == 0) {
	printf("%4d", a);
	cnt++;
	}
	printf("\n");
	cnt = 0;
      return 0;
      }
    
 2.
#pragma warning (disable : 4996)
#include <stdio.h>
    int main()
{
	//절대값 출력하는 프로그램을 만드시오.
	// -5 -> 5, 5 -> 5
	int a, abs;
	scnaf("%d", &a);
	if (a < 0)
		abs = a * -1;
	else
		abs = a;
	printf("절대값 : %d\n", abs);
	// 3황 연산자 ? :
	abs = a < 0 ? a * -1 : a;
	printf("절대값 : %d\n", abs);
	return 0;
}

3.
#pragma warning (disable : 4996)
#include <stdio.h>
int main()
{
	//두 정수를 입력받아 그 차이를 출력
	//예를 들어 5 9 입력받으면 4가 출력
	//그리고 9 5를 입력받아도 4가 출력
	//항상 양의 차이값이 출력되도록 하시오.
	int n1 , n2, abs;
	scnaf("%d %d", &n1, &n2);
	if (n1 > n2)
		abs = n1 - n2;
	else
		abs = n2 - n1;
	printf("차이 : %d\n", abs);

	return 0;
}

