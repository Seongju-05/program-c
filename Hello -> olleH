#pragma warning (disable : 4996)
#include <stdio.h>
//문자열을 입력 받아 문자열을 뒤집어 출력하는
//프로그램을 만드시오.
// 입력 예: Hello
// 출력 : olleH
//힌트 : 널문자,배열의 인덱스 확용
int main()
{
	char str[100];
	int i = 0;
	scanf("%s", str);
	while (str[i] != '\0')
	{
		i++;
	}
	i--;
	for (; i >= 0; i--)
	{
		printf("%c", str[i]);
	}
		return 0;
	}
