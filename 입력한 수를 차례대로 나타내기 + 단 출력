#pragma (disable : 4996)
#include <stdio.h>

int main()
{
	int b, c;
	printf("수를 입력하세요: ");
	scanf("%d", &c);
	b = 0;
	for (b; b < c; b++) {
		printf("%d\n", b);
	}
	return 0;
}



#pragma (disable : 4996)
#include <stdio.h>
int main()
{
	int i, x;
	printf("단을 입력하세요: ");
	scanf("%d", &x);
	for (i = 1; i < 10; i++)
	{
		printf("%d * %d = %d\n", x, i, x * i);
	}
	return 0;
}
